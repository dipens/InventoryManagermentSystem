<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="app.datagrid.builder.W3viewItemsList" class="Inventory\RegularInventoryBundle\W3viewItemsListBuilder" >
            <argument type="service" id="thrace_data_grid.factory.datagrid" />
            <argument type="service" id="translator" />
            <argument type="service" id="router" />
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument type="service" id="request_stack" />
        </service>

        <service id="app.datagrid.W3viewItemsList" class="Thrace\DataGridBundle\DataGrid\DataGrid">
            <factory service="app.datagrid.builder.W3viewItemsList" method="build" />
            <tag name="thrace_data_grid.datagrid" alias="W3viewItemsList" />
        </service>

